  "description": "Cypress DOM snapshots and consistent image diffing in the cloud",
  "main": "src",
  "types": "src",
  "files": [
    "src",
    "plugin"
  ],
  "scripts": {
    "test": "start-test 3777 cy:run",
    "start": "serve -p 3777 frontend-todo-app",
